@model IEnumerable<Omniaccounts.Models.Stock_Export>

@{
    ViewBag.Title = "GetStock";
}
<div class="page-wrapper">
    <!-- HEADER DESKTOP-->


    <div class="page-content--bgf7">

        <!-- WELCOME-->
        <section class="welcome2 p-t-40 p-b-55">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="au-breadcrumb3">
                            <div class="au-breadcrumb-left">
                                
                              
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="welcome2-inner m-t-60">
                            <div class="welcome2-greeting">
                                <h1 class="title-6">
                                     Welcome
                                </h1>
                                <p>Below are details of a product you have searched</p>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- END WELCOME-->
        <section class="welcome p-t-10">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">

                        <br />

                        @foreach (var item in Model)
                        {
                            <div class="row">

                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.product_group_description):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.product_group_description)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.stock_category_description):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.stock_category_description)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.stock_code):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.stock_code)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.stock_description):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.stock_description)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.unit_of_measure):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.unit_of_measure)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.pack):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.pack)
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.bar_code):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.bar_code)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.incl_unit_selling_price):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.incl_unit_selling_price)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                                <div class="col-lg-4">
                                    <section class="card">
                                        <div class="card-body text-secondary">
                                            <div>
                                                <strong>@Html.DisplayNameFor(model => model.level):</strong>
                                            </div>
                                            <div>
                                                @Html.DisplayFor(modelItem => item.level)
                                            </div>

                                        </div>
                                    </section>
                                </div>
                            </div>

                        }

                        <div>
                            @Html.ActionLink("Back to List", "Index", "Home")
                        </div>


                    </div>
                </div>
            </div>
        </section>
    </div>
</div>




